{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/MagicSpritz.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/MagicSpritz.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/MagicSpritz.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Generate MagicOnion Client",
            "command": "./tools/moc/osx-x64/moc",
            "type": "process",
            "args": [
                "-i",
                "${workspaceFolder}/samples/RM.Hotel/Shared/RM.Hotel.Shared.csproj",
                "-o",
                "${workspaceFolder}/samples/RM.Hotel/Client/Generated/RM.Hotel.Generated.cs"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Generate MessagePack",
            "command": "./tools/mpc/osx/mpc",
            "type": "process",
            "args": [
                "-i",
                "${workspaceFolder}/samples/RM.Hotel/Shared/RM.Hotel.Shared.csproj",
                "-o",
                "${workspaceFolder}/samples/RM.Hotel/Shared/Generated/MessagePackGenerated.cs"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}